{
  "name": "TheRxSpot_Marketplace",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "postinstall": "node ./scripts/ensure-date-fns-locale.mjs",
    "build": "medusa build",
    "db:clean-migrate": "node ./scripts/db/clean-migrate.mjs",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "seed:therxspot": "medusa exec ./src/scripts/seed-the-rx-spot.ts",
    "start": "medusa start",
    "dev": "medusa develop",
    "test": "npm run test:integration:http",
    "test:integration:http": "cross-env TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "cross-env TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:custom": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config=jest.integration.config.js --silent=false --runInBand --forceExit",
    "test:unit": "cross-env TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit --passWithNoTests",
    "test:consult-gating": "cross-env NODE_OPTIONS=--experimental-vm-modules jest src/tests/integration/consult-gating.test.ts --silent=false --runInBand --forceExit",
    "test:consultation": "cross-env NODE_OPTIONS=--experimental-vm-modules jest src/tests/integration/consultation-lifecycle.test.ts --silent=false --runInBand --forceExit",
    "test:earnings": "cross-env NODE_OPTIONS=--experimental-vm-modules jest src/tests/integration/earnings.test.ts --silent=false --runInBand --forceExit",
    "test:documents": "cross-env NODE_OPTIONS=--experimental-vm-modules jest src/tests/integration/documents.test.ts --silent=false --runInBand --forceExit",
    "test:audit": "cross-env NODE_OPTIONS=--experimental-vm-modules jest src/tests/integration/audit-logging.test.ts --silent=false --runInBand --forceExit",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "diagnose:smoke": "powershell -ExecutionPolicy Bypass -File ./scripts/smoke/verify-local.ps1",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" \"integration-tests/**/*.{ts,tsx,js,jsx}\" \"scripts/**/*.{ts,tsx,js,jsx,mjs,cjs}\" \"*.{js,mjs,cjs,ts}\"",
    "storefront:check": "npm -C TheRxSpot_Marketplace-storefront run check",
    "check": "npm run lint && npm run typecheck && npm run build && npm run storefront:check"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "2.13.1",
    "@medusajs/cli": "2.13.1",
    "@medusajs/framework": "2.13.1",
    "@medusajs/medusa": "2.13.1",
    "@types/qrcode": "^1.5.6",
    "clamscan": "^1.4.2",
    "date-fns": "^3.6.0",
    "file-type": "^19.6.0",
    "ioredis": "^5.3.2",
    "pino": "^9.5.0",
    "qrcode": "^1.5.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@medusajs/test-utils": "2.13.1",
    "@swc/core": "^1.7.28",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.2",
    "globals": "^17.3.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^5.4.14",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "npm@11.5.2"
}
